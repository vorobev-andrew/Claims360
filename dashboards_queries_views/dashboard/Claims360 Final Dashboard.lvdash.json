        "ORDER BY month, payer_name;"
      ]
    },
    {
      "name": "da2e9aa7",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `claims360_dev`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "SELECT\n",
        "  COALESCE(m.reason_category, 'Unmapped') AS denial_category,\n",
        "  ROUND(SUM(-1 * COALESCE(e.adjustment_amount,0)), 2) AS denial_amount\n",
        "FROM gold.fact_denial_event e\n",
        "LEFT JOIN silver.dim_denial_reason_map m\n",
        "  ON m.code_type = 'CARC' AND m.code = e.reason_code\n",
        "GROUP BY 1\n",
        "ORDER BY denial_amount DESC\n",
        "LIMIT 8;"
      ]
    },
    {
      "name": "6425367c",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `claims360_dev`;\n",
        "USE SCHEMA `default`;\n",
