    {
      "name": "d959f53b",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `claims360_dev`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "WITH base AS (\n",
        "  SELECT\n",
        "    SUM(CASE WHEN current_balance <= 0 AND has_denial_any = 0 AND had_277_reject = 0 THEN 1 ELSE 0 END) AS fpy_claims,\n",
        "    COUNT(*) AS submitted_claims\n",
        "  FROM gold.fact_claim\n",
        ")\n",
        "SELECT CASE WHEN submitted_claims = 0 THEN 0\n",
        "            ELSE ROUND(fpy_claims * 100.0 / submitted_claims, 2)\n",
        "       END AS fpy_pct\n",
        "FROM base;"
      ]
    },
    {
      "name": "f1a0643a",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `claims360_dev`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "WITH base AS (\n",
        "  SELECT\n",
        "    SUM(CASE WHEN has_denial_any = 1 THEN 1 ELSE 0 END) AS denied_claims,\n",
        "    COUNT(*) AS submitted_claims\n",
        "  FROM gold.fact_claim\n",
        ")\n",
        "SELECT CASE WHEN submitted_claims = 0 THEN 0\n",
        "            ELSE ROUND(denied_claims * 100.0 / submitted_claims, 2)\n",
        "       END AS denial_rate_pct\n",
        "FROM base;"
      ]
    },
    {
      "name": "641d1820",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `claims360_dev`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "SELECT ROUND(SUM(-1 * COALESCE(adjustment_amount,0)), 2) AS adjustment_impact_usd\n",
        "FROM gold.fact_denial_event\n",
        "WHERE event_ts <= date_sub(current_date(), 30);"
      ]
    },
    {
      "name": "713ae534",
