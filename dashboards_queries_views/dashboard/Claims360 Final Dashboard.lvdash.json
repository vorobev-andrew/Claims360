      ]
    },
    {
      "name": "96dd1b3c",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `claims360_dev`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "WITH first_pmt AS (\n",
        "  SELECT claim_id, MIN(posted_ts) AS first_payment_ts\n",
        "  FROM silver.payments_835_events\n",
        "  GROUP BY claim_id\n",
        ")\n",
        "SELECT\n",
        "  c.payer_name,\n",
        "  ABS(ROUND(AVG(datediff(DAY, c.submission_date, f.first_payment_ts)), 1)) AS avg_days_to_first_payment\n",
        "FROM gold.fact_claim c\n",
        "LEFT JOIN first_pmt f USING (claim_id)\n",
        "GROUP BY c.payer_name\n",
        "ORDER BY avg_days_to_first_payment DESC;\n"
      ]
    },
    {
      "name": "cc9a09d7",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `claims360_dev`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "WITH first_pmt AS (\n",
        "  SELECT claim_id, MIN(posted_ts) AS first_payment_ts\n",
        "  FROM silver.payments_835_events\n",
        "  GROUP BY claim_id\n",
        ")\n",
        "SELECT\n",
        "  c.payer_name,\n",
        "  ABS(ROUND(AVG(datediff(DAY, c.submission_date, f.first_payment_ts)), 1)) AS avg_days_to_first_payment\n",
        "FROM gold.fact_claim c\n",
        "LEFT JOIN first_pmt f USING (claim_id)\n",
        "GROUP BY c.payer_name\n",
        "ORDER BY avg_days_to_first_payment DESC;\n"
      ]
    },
    {
      "name": "74f243ce",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `claims360_dev`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "SELECT\n",
        "  payer_name,\n",
        "  ROUND(SUM(billed_amount),2)    AS billed,\n",
        "  ROUND(SUM(net_paid_to_date),2) AS paid,\n",
        "  ROUND(SUM(current_balance),2)  AS balance\n",
        "FROM gold.fact_claim\n",
        "GROUP BY payer_name\n",
        "ORDER BY billed DESC;"
      ]
    },
    {
      "name": "3cd9a5f7",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `claims360_dev`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "SELECT\n",
        "  payer_name,\n",
        "  reason_category,\n",
        "  ROUND(SUM(-1 * COALESCE(adjustment_amount,0)), 2) AS adjustment_impact_usd\n",
        "FROM gold.fact_denial_event\n",
        "GROUP BY payer_name, reason_category\n",
        "ORDER BY adjustment_impact_usd DESC;"
      ]
    }
