    {
      "name": "e45cb0f0",
      "displayName": "v_monthly_aging",
      "queryLines": [
        "USE CATALOG `claims360_dev`;\n",
        "\n",
        "SELECT\n",
        "  payer_name,\n",
        "  aging_bucket,\n",
        "  CASE aging_bucket\n",
        "    WHEN '0–30' THEN 1 WHEN '31–60' THEN 2 WHEN '61–90' THEN 3\n",
        "    WHEN '91–120' THEN 4 WHEN '120+' THEN 5 ELSE 0\n",
        "  END AS bucket_order,\n",
        "  ar_amount\n",
        "FROM claims360_dev.gold.v_aging_buckets\n",
        "WHERE aging_bucket <> 'Closed';"
      ],
      "catalog": "claims360dbx"
    },
    {
      "name": "062f2076",
      "displayName": "v_monthly_aging_bubble",
      "queryLines": [
        "USE CATALOG `claims360_dev`;\n",
        "\n",
        "WITH pivot AS (\n",
        "  SELECT payer_name,\n",
        "         SUM(ar_amount) AS ar_total,\n",
        "         SUM(CASE WHEN aging_bucket='120+' THEN ar_amount END) AS ar_120p,\n",
        "         SUM(claim_count) AS claims\n",
        "  FROM claims360_dev.gold.v_aging_buckets\n",
        "  WHERE aging_bucket <> 'Closed'\n",
        "  GROUP BY payer_name\n",
        ")\n",
        "SELECT payer_name, ar_total, ar_120p, claims FROM pivot;"
      ],
      "catalog": "claims360dbx"
    },
    {
      "name": "12a5fb06",
      "displayName": "v_monthly_aging",
      "queryLines": [
        "USE CATALOG `claims360dbx`;\n",
