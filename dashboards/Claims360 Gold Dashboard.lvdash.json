        "    WHEN '91–120' THEN 4 WHEN '120+' THEN 5 ELSE 0\n",
        "  END AS bucket_order,\n",
        "  ar_amount\n",
        "FROM claims360_dev.gold.v_aging_buckets\n",
        "WHERE aging_bucket <> 'Closed';"
      ],
      "catalog": "claims360dbx"
    }
